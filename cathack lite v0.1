javascript:(function(){
  const container = document.createElement("div");
  container.style.cssText = `
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background-color: #0F0;
    color: #000;
    width: 200px;
    padding: 10px;
    text-align: center;
    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);
    z-index: 999999;
  `;
  
  const title = document.createElement("h1");
  title.innerText = "cathack lite";
  title.style.marginTop = "0";
  container.appendChild(title);
  
  const select = document.createElement("select");
  select.style.width = "100%";
  const options = [
    {label: "1", url: "url1"},
    {label: "2", url: "url2"},
    {label: "3", url: "url3"},
    {label: "4", url: "url4"},
  ];
  options.forEach((option) => {
    const opt = document.createElement("option");
    opt.value = option.url;
    opt.text = option.label;
    select.appendChild(opt);
  });
  container.appendChild(select);
  
  const button = document.createElement("button");
  button.innerText = "Submit";
  button.style.width = "100%";
  button.style.marginTop = "10px";
  button.addEventListener("click", () => {
    container.removeChild(select);
    container.removeChild(button);
    
    const progress = document.createElement("progress");
    progress.style.width = "100%";
    container.appendChild(progress);
    
    const status = document.createElement("div");
    status.innerText = "fetching URL...";
    container.appendChild(status);
    
    const url = select.value;
    fetch(url)
      .then((response) => {
        if (response.ok) {
          return response.text();
        } else {
          throw new Error("Failed to fetch");
        }
      })
      .then((code) => {
        status.innerText = "decompiling code...";
        
        setTimeout(() => {
          status.innerText = "preparing to execute...";
          
          setTimeout(() => {
            status.innerText = "executed ✅";
            
            setTimeout(() => {
              status.innerText = "closing menu...";
              
              setTimeout(() => {
                document.body.removeChild(container);
              }, 1000);
            }, 1000);
          }, 1000);
        }, 1000);
      })
      .catch((error) => {
        status.innerText = "failed to load!!! ❌";
        
        setTimeout(() => {
          status.innerText = "closing UI...";
          
          setTimeout(() => {
            document.body.removeChild(container);
          }, 1000);
        }, 1000);
      });
  });
  container.appendChild(button);
  
  document.body.appendChild(container);
})();
