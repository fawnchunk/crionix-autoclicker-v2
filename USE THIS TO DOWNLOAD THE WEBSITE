javascript:(function(){var html = prompt('Enter the HTML code:');if(html !== null){var file = new Blob([html], {type: 'text/html'});var url = URL.createObjectURL(file);var a = document.createElement('a');a.href = url;a.download = 'my-page.html';document.body.appendChild(a);a.click();document.body.removeChild(a);URL.revokeObjectURL(url);}})();
